<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="PopupWindowAnimation" />
    <style name="PopupWindowAnimation.Complex">
        <item name="android:windowEnterAnimation">@anim/popup_window_enter_complex</item>
        <item name="android:windowExitAnimation">@anim/popup_window_exit_complex</item>
    </style>
    <style name="PopupWindowAnimation.Slide">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_top</item>
        <item name="android:windowExitAnimation">@anim/slide_out_to_bottom</item>
    </style>
    
    <style name="DefaultButton" parent="YFUi.Widget.Button.Outlined.Info">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginHorizontal" tools:targetApi="o">@dimen/common_margin_horizontal</item>
    </style>
    
</resources>